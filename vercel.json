{
  "version": 2,
  "functions": {
    "api/*.php": { "runtime": "vercel-php@0.6.0" }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/connexion.php"
    },
    {
      "src": "/connexion",
      "dest": "/api/connexion.php"
    },
    {
      "src": "/inscription",
      "dest": "/api/inscription.php"
    },
    {
      "src": "/home",
      "dest": "/api/home.php"
    },
    {
      "src": "/creation_article",
      "dest": "/api/creation_article.php"
    },
    {
      "src": "/profil_user",
      "dest": "/api/profil_user.php"
    },
    {
      "src": "/profil",
      "dest": "/api/profil.php"
    },
    {
      "src": "/view_article",
      "dest": "/api/view_article.php"
    },
    {
      "src": "/public/(.*)",
      "dest": "/$1"
    }
  ]
}
