{
  "version": 2,
  "functions": {
    "api/*.php": { "runtime": "vercel-php@0.6.0" }
  },
  "routes": [
    {
      "src": "/api/home.php",
      "headers": { "Location": "/home" },
      "status": 307
    },
    {
      "src": "/",
      "dest": "/api/connexion.php"
    },
    {
      "src": "/home",
      "dest": "/api/home.php"
    },
    {
      "src": "/public/(.*)",
      "dest": "/$1"
    }
  ]
}